  "scripts": {
    "start": "bash -ac '. .env.${REACT_APP_ENV}; react-app-rewired start",
    "build": "bash -ac '. .env.${REACT_APP_ENV}; react-app-rewired build",
    "test": "react-app-rewired test --silent --noStackTrace --colors",
    "start:xpollinate": "REACT_APP_ENV=xpollinate-mainnet.development npm run start",
    "start:xpollinate:testnet": "REACT_APP_ENV=xpollinate-testnet.development npm run start",
    "build:lifinance:staging": "REACT_APP_ENV=lifinance-mainnet.staging npm run build",
    "build:lifinance": "REACT_APP_ENV=lifinance-mainnet npm run build",
    "build:xpollinate": "REACT_APP_ENV=xpollinate-mainnet npm run build",
    "build:xpollinate:staging": "REACT_APP_ENV=xpollinate-mainnet.staging npm run build",
    "build:xpollinate:testnet": "REACT_APP_ENV=xpollinate-testnet npm run build",
    "build:xpollinate:testnet:staging": "REACT_APP_ENV=xpollinate-testnet.staging npm run build",
    "test:ci": "echo 'Not defined'",
    "storybook": "start-storybook -p 6006 -s public",
    "pre-commit": "lint-staged",
    "lint:fix": "eslint --ext .tsx --ext .ts ./src --fix",
    "prettier:fix": "prettier --write ./src/."
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn run lint:fix",
      "yarn run prettier:fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^14"
  }
