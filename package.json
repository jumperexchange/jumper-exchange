{
  "name": "lifi-web",
  "version": "0.1.0",
  "homepage": "https://li.finance",
  "bugs": "https://github.com/lifinance/lifi-web/issues",
  "license": "UNLICENSED",
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@lifinance/sdk": "^0.4.2",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.1.9",
    "antd": "^4.16.0",
    "motion": "^10.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-responsive": "^9.0.0-beta.4",
    "react-router-dom": "^5.2.0",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.6.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "lint-staged": ">=10",
    "prettier": "2.4.1",
    "react-scripts": "4.0.3",
    "typescript": "4.3.5",
    "yarn": ">=1.22"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:stagingBackend": "bash -ac '. .env.development.stagingBackend; react-scripts start'",
    "start-env": "bash -ac '. .env.${REACT_APP_ENV}; react-scripts start'",
    "build": "react-scripts build",
    "build-env": "bash -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "build:lifinance:staging": "REACT_APP_ENV=lifinance-mainnet.staging npm run build-env",
    "build:lifinance:develop": "REACT_APP_ENV=lifinance-mainnet.develop npm run build-env",
    "build:lifinance": "REACT_APP_ENV=lifinance-mainnet npm run build-env",
    "test": "react-scripts test",
    "pre-push": "react-scripts test unit --watchAll=false --passWithNoTests",
    "pre-commit": "lint-staged",
    "lint:fix": "eslint --ext .tsx --ext .ts ./src --fix",
    "prettier:fix": "prettier --write ./src/.",
    "sdk:link": "yarn link '@lifinance/sdk'",
    "sdk:unlink": "yarn unlink '@lifinance/sdk' && yarn sdk:update",
    "sdk:update": "yarn install --force",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn run lint:fix",
      "yarn run prettier:fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
