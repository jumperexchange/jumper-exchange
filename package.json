  "packageManager": "yarn@4.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./index.d.ts",
  "dependencies": {
    "@arcxmoney/analytics": "^2.0.3",
    "@cookie3/analytics": "^0.6.0",
    "@lifi/sdk": "^3.0.0-alpha.56",
    "@lifi/wallet-management": "3.0.0-alpha.22",
    "@lifi/widget": "3.0.0-alpha.32",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@safe-global/safe-apps-sdk": "^9.0.0",
    "@sentry/browser": "^7.101.1",
    "@sentry/integrations": "^7.101.1",
    "@sentry/react": "^7.101.1",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-wallets": "^0.19.26",
    "@solana/web3.js": "^1.90.0",
    "@strapi/blocks-react-renderer": "^1.0.0",
    "@tanstack/react-query": "5.20.5",
    "@ungap/structured-clone": "^1.2.0",
    "@widgetbot/react-embed": "^1.9.0",
    "fontaine": "^0.4.1",
    "i18next": "^23.8.2",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-resources-to-backend": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-ga4": "^2.1.0",
    "react-hotjar": "^6.2.0",
    "react-i18next": "^14.0.5",
    "react-markdown": "^9.0.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.0",
    "react-swipeable": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "remark-text-decoration": "^1.1.0",
    "typescript": "^5.3.3",
    "viem": "^2.7.9",
    "vite-tsconfig-paths": "^4.3.1",
    "wagmi": "2.5.7",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@lifi/types": "^11.3.0",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/ungap__structured-clone": "^1.2.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "web-vitals": "^3.5.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode localhost",
    "dev:testnet": "vite --mode testnet",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "tsc && vite build",
    "build:testnet": "tsc && vite build --mode testnet",
    "build:develop": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "clean": "rm -rf ./tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint-staged": "lint-staged",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "run-s 'test:*'",
    "test:unit": "echo \"No tests yet\"",
    "fix:staged-files": "lint-staged --allow-empty",
    "fix:all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "pre-commit": "lint-staged",
    "pre-push": "yarn && yarn build",
    "start": "tsc --build --clean && tsc --build && tsc --build tsconfig.json"
  },
  "lint-staged": {
    "**/*.(ts|tsx|js)": [
      "bash -c tsc --noEmit",
      "yarn eslint --fix",
      "yarn prettier --write"
    ],
    "**/*.(md|json)": [
      "yarn prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
