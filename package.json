{
  "name": "lifi-web",
  "version": "0.1.0",
  "homepage": "https://transferto.xyz",
  "bugs": "https://github.com/lifinance/lifi-web/issues",
  "license": "UNLICENSED",
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@etherspot/contracts": "^1.8.0",
    "@lifi/sdk": "^1.2.2",
    "@lifi/widget": "^1.13.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^7.0.2-alpha.0",
    "antd": "^4.22.4",
    "etherspot": "^1.35.2",
    "motion": "^10.13.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.0-beta.10",
    "react-router-dom": "^6.3.0",
    "react-twitter-embed": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "web-vitals": "^3.0.0-rc.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.21.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": ">=13.0.3",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.4",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "start:developBackend": "GENERATE_SOURCEMAP=false bash -ac '. .env.development.developBackend; react-scripts start'",
    "start-env": "bash -ac '. .env.${REACT_APP_ENV}; react-scripts start'",
    "build": "react-scripts build",
    "build-env": "bash -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "build:lifinance:staging": "REACT_APP_ENV=lifinance-mainnet.staging npm run build-env",
    "build:lifinance:develop": "REACT_APP_ENV=lifinance-mainnet.develop npm run build-env",
    "build:lifinance": "REACT_APP_ENV=lifinance-mainnet npm run build-env",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(@lifi/sdk))/\"",
    "pre-push": "react-scripts test unit --watchAll=false --passWithNoTests --transformIgnorePatterns \"node_modules/(?!(@lifi/sdk))/\"",
    "pre-commit": "lint-staged",
    "lint:fix": "eslint --ext .tsx --ext .ts ./src --fix",
    "prettier:fix": "prettier --write ./src/.",
    "sdk:link": "yarn link '@lifi/sdk'",
    "sdk:unlink": "yarn unlink '@lifi/sdk' && yarn sdk:update",
    "sdk:update": "yarn install --force",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn run lint:fix",
      "yarn run prettier:fix"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
